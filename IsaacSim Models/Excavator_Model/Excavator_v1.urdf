  <joint
    name="revolute_cabin"
    type="revolute">
    <origin
    xyz="0 0 0"
    rpy="0 0 0 " />
    <parent
      link="undercarriage" />
    <child
      link="uppercarriage" />
    <axis
      xyz="0 0 1" />
    <limit
      lower="0"
      upper="0"
      effort="5"
      velocity="0.3" />
    <dynamics
      damping="0.05"
      friction="0" />
  </joint>

    <!--
    Revolute_lift angle limit values, lower "0.1745329252" , upper "1.6057029118", angle value in radians.
    Revolute_lift kulman raja-arvot, lower "0.1745329252" , upper "1.6057029118", kulma-arvo radiaaneina.
    Test_bench_position_control.urdf needs stiffness to be higher and dambing lower, values in isaac sim : Damping 0.05 and Stiffness 1.0    
    -->
  
  <joint
    name="revolute_lift"
    type="revolute">
    <origin
      xyz="0.034 0 0.0625"
      rpy="0 0 0 " />
    <parent
      link="uppercarriage" />
    <child
      link="liftboom" />
    <axis
      xyz="0 1 0" />
    <limit
      lower="0.1745329252"
      upper=".6057029118" 
      effort="60"
      velocity="0" />
    <dynamics
      damping="0.05"
      friction="0.5" /> 
  </joint>

